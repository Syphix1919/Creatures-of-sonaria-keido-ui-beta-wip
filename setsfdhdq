	Followup = false,
	Title = "Welcome",
	Text = "Select an automatism you want to activate here",
	Buttons = {
		Water = function()
			autoDrink()
		end,
		Sniff = function()
			autoSniff()
		end,
		NoThanks = function()
		
		end
	}
}

