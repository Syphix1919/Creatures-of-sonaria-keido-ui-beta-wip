local creatureSlot
 Tab8:Dropdown{
	Name = "Dropdown",
	StartingText = "Select...",
	Description = nil,
	Items = {
		{"Slot1", "Slot1"}, 
		{"Slot2", "Slot2"}, 
		{"Slot3", "Slot3"}, 

	},
	Callback = function(itemSlotOfCreature) 
		creatureSlot = itemSlotOfCreature
	return end
 }

 Tab8:Button{
	Name = "Slot notif",
	Description = "",
	Callback = function() 
		GUI:Notification{
			Title = "Here :",
			Text = creatureSlot,
			Duration = 3,
			Callback = function() end
		}
	end
}
